<body>
  <div class="wrap">
    <main class="card">

      <!-- LOGOMARCA PROTAGONISTA -->
      <header>
        <img id="logoImg" alt="Logomarca do Banco do Nordeste">
      </header>

      <!-- TELA INICIAL -->
      <section id="screen-home" class="screen active">
        <h1>Clique e descubra qual será o formulário a ser respondido para concorrer a prêmios!</h1>
        <p>Toque no botão abaixo para iniciar o sorteio.</p>
        <button id="btnStart">Iniciar</button>
      </section>

      <!-- TELA DE CONTAGEM -->
      <section id="screen-countdown" class="screen">

        <div class="formHighlight">
          <div class="formTitle">Formulário</div>
          <div id="formCode" class="formCode">V-- – -</div>
          <div id="lineForm" class="formLead">Redirecionando…</div>
        </div>

        <div class="kpi">
          <div id="secLeft" class="big">5</div>
          <div class="label">segundos</div>
        </div>

        <div class="progress">
          <div id="bar" class="bar"></div>
        </div>

      </section>
    </main>
  </div>


  <script>
    /* COLE A SUA LOGO (apenas o Base64 puro) */
    const LOGO_BASE64 = "";

    document.getElementById("logoImg").src =
      "data:image/png;base64," + LOGO_BASE64;

    const COUNTDOWN_SECONDS = 5;

    const FORMS = [
      { id:"V01",letter:"A",url:"https://forms.office.com/r/GZpZWVG6Re" },
      { id:"V02",letter:"B",url:"https://forms.office.com/r/pv7rncruHh" },
      { id:"V03",letter:"C",url:"https://forms.office.com/r/RbUe2mKKG8" },
      { id:"V04",letter:"D",url:"https://forms.office.com/r/CbRjthVpM3" },
      { id:"V05",letter:"E",url:"https://forms.office.com/r/97xwnEZvgg" },
      { id:"V06",letter:"F",url:"https://forms.office.com/r/vSfq9HbcvG" },
      { id:"V07",letter:"G",url:"https://forms.office.com/r/iMhVbteRur" },
      { id:"V08",letter:"H",url:"https://forms.office.com/r/81HjBjpCwS" },
      { id:"V09",letter:"I",url:"https://forms.office.com/r/nJGm85dLXr" },
      { id:"V10",letter:"J",url:"https://forms.office.com/r/M5Egw2JL8m" },
      { id:"V11",letter:"K",url:"https://forms.office.com/r/UM7yMbfT2j" },
      { id:"V12",letter:"L",url:"https://forms.office.com/r/WEA2c6F355" },
      { id:"V13",letter:"M",url:"https://forms.office.com/r/4netnbKZC8" },
      { id:"V14",letter:"N",url:"https://forms.office.com/r/XavgAVgCBF" },
      { id:"V15",letter:"O",url:"https://forms.office.com/r/s09jYVb8ir" },
      { id:"V16",letter:"P",url:"https://forms.office.com/r/iUQkr0BnXE" }
    ];

    const screenHome = document.getElementById("screen-home");
    const screenCountdown = document.getElementById("screen-countdown");
    const btnStart = document.getElementById("btnStart");
    const formCode = document.getElementById("formCode");
    const lineForm = document.getElementById("lineForm");
    const secLeft = document.getElementById("secLeft");
    const bar = document.getElementById("bar");

    function randomInt(min,max){
      const range=max-min+1;
      if(crypto.getRandomValues){
        let x;
        const u=new Uint32Array(1);
        const ub=Math.floor(0xFFFFFFFF/range)*range;
        do{crypto.getRandomValues(u);x=u[0];}while(x>=ub);
        return min+(x%range);
      }
      return Math.floor(Math.random()*range)+min;
    }

    function drawForm(){
      return FORMS[randomInt(0,FORMS.length-1)];
    }

    let running=false;
    let selected=null;
    let startTs=0;
    let rafId=0;

    function setScreen(home){
      if(home){
        screenCountdown.classList.remove("active");
        screenHome.classList.add("active");
      }else{
        screenHome.classList.remove("active");
        screenCountdown.classList.add("active");
      }
    }

    function resetUI(){
      cancelAnimationFrame(rafId);
      running=false;
      secLeft.textContent=COUNTDOWN_SECONDS;
      bar.style.width="0%";
      formCode.textContent="V-- – -";
      lineForm.textContent="Redirecionando…";
      btnStart.disabled=false;
      setScreen(true);
    }

    function tick(now){
      const elapsed=(now-startTs)/1000;
      const remaining=Math.max(0,COUNTDOWN_SECONDS-elapsed);
      secLeft.textContent=Math.ceil(remaining);
      bar.style.width=((elapsed/COUNTDOWN_SECONDS)*100)+"%";

      if(remaining<=0){
        window.location.href=selected.url;
        return;
      }
      rafId=requestAnimationFrame(tick);
    }

    btnStart.addEventListener("click",()=>{
      if(running) return;
      running=true;
      btnStart.disabled=true;

      selected=drawForm();
      formCode.textContent=`${selected.id} – ${selected.letter}`;
      lineForm.textContent=`Redirecionando em ${COUNTDOWN_SECONDS} segundos…`;

      setScreen(false);
      startTs=performance.now();
      rafId=requestAnimationFrame(tick);
    });

    window.addEventListener("pageshow", resetUI);
    resetUI();
  </script>

</body>
</html>
