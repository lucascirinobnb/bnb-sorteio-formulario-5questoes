      background:
        radial-gradient(1100px 850px at 15% 15%, rgba(242,116,5,.22), transparent 60%),
        radial-gradient(1100px 850px at 85% 25%, rgba(166,23,61,.30), transparent 60%),
        radial-gradient(900px 650px at 55% 95%, rgba(89,89,89,.28), transparent 55%),
        linear-gradient(135deg,#14080a,#0c0c0f);
    }

    .wrap{
      min-height:100%;
      display:flex;
      align-items:center;
      justify-content:center;
      padding: clamp(16px, 3vw, 36px);
    }

    .card{
      width:min(980px,96vw);
      background:linear-gradient(180deg,var(--card),var(--card2));
