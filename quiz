    }

    button{
      width:min(560px,90vw);
      padding:24px 28px;
      border:none;
      border-radius:18px;
      font-size:clamp(22px,2.6vw,32px);
      font-weight:900;
      color:#fff;
      background:linear-gradient(135deg,var(--bn-primary),var(--bn-secondary));
